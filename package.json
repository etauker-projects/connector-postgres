{
  "name": "@etauker/connector-postgres",
  "version": "1.0.1",
  "description": "Node postgres connector",
  "main": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "module": "esm",
  "type": "module",
  "engines": {
    "node": "^16.8.0"
  },
  "scripts": {
    "prelint": "",
    "lint": "echo '--- [CONNECTOR POSTGRES - WARNING] lint script not defined'",
    "postlint": "",
    "preinstall": "",
    "install": "",
    "postinstall": "",
    "prebuild": "npx rimraf dist",
    "build": "tsc --project ./config/tsconfig.json",
    "postbuild": "",
    "pretest": "",
    "test": "TS_NODE_PROJECT='./config/tsconfig.json' ts-mocha -r esm \"src/**/*.spec.ts\"",
    "preintegration": "",
    "integration": "TS_NODE_PROJECT='./config/tsconfig.json' node --loader ts-node/esm --es-module-specifier-resolution=node test/integration.ts",
    "posttest": "",
    "prestart": "",
    "start": "TS_NODE_PROJECT='./config/tsconfig.json' node --loader ts-node/esm --es-module-specifier-resolution=node src/index.ts",
    "poststart": "",
    "predeploy:patch": "npm run lint && npm install && npm run build && npm run test && npm version patch",
    "deploy:patch": "npm publish",
    "postdeploy:patch": "git push --follow-tags",
    "predeploy:minor": "npm run lint && npm install && npm run build && npm run test && npm version minor",
    "deploy:minor": "npm publish",
    "postdeploy:minor": "git push --follow-tags",
    "predeploy:major": "npm run lint && npm install && npm run build && npm run test && npm version major",
    "deploy:major": "npm publish",
    "postdeploy:major": "git push --follow-tags"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/etauker/connector-postgres.git"
  },
  "keywords": [
    "etauker",
    "node",
    "postgres"
  ],
  "author": {
    "name": "Tautvydas Kersulis",
    "email": "etauker@gmail.com"
  },
  "license": "UNLICENCED",
  "bugs": {
    "url": "https://github.com/etauker/connector-postgres/issues"
  },
  "homepage": "https://github.com/etauker/connector-postgres#readme",
  "dependencies": {
    "esm": "^3.2.25",
    "mocha": "^8.4.0",
    "pg": "^8.6.0",
    "pg-pool": "^3.3.0",
    "sinon": "^11.1.2",
    "ts-mocha": "^8.0.0",
    "typescript": "^4.3.5"
  },
  "devDependencies": {
    "@types/mocha": "^9.0.0",
    "@types/node": "^16.3.3",
    "@types/pg": "^8.6.1",
    "@types/pg-pool": "^2.0.3",
    "@types/sinon": "^10.0.6",
    "rimraf": "^3.0.2",
    "ts-node": "^10.1.0"
  }
}
